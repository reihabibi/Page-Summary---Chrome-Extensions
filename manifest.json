{
  "name": "Page Summary Generator",
  "version": "0.1.0",
  "description": "A simple page summary generator",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_title": "Summary Generator",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}



